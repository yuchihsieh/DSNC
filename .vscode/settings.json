{
  "latex-workshop.latex.outDir": "build",
  "latex-workshop.latex.recipes": [
    { "name": "latexmk (pdf)", "tools": ["latexmk"] }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-interaction=nonstopmode",
        "-halt-on-error",
        "-outdir=build",
        "%DOC%"
      ]
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.color.dark.backgroundColor": "#D4D4D4",
  "latex-workshop.view.pdf.color.dark.pageColorsBackground": "#1E1E1E",
  "latex-workshop.view.pdf.color.dark.pageColorsForeground": "#D4D4D4",
  "latex-workshop.view.pdf.color.dark.pageBorderColor": "#333333"
}
